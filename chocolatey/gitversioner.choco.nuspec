<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>GitVersioner</id>
    <version>1.0.2</version>
    <title>ArachNGIN GitVersioner</title>
    <authors>Michal Kuncl</authors>
    <projectUrl>https://github.com/Pavuucek/GitVersioner</projectUrl>
    <licenseUrl>https://github.com/Pavuucek/GitVersioner/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>Git, Versioning, Tool</tags>
    <description>A small tool to get version info from git and update AssemblyInfo.cs.</description>
    <dependencies>
      <dependency id="git" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\bin\Release\*.*" target="tools" />
	<file src="tools\*.*" target="tools" />
  </files>
</package>
